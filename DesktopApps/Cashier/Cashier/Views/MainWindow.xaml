<Window x:Class="Cashier.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
        xmlns:local="clr-namespace:Cashier.Views"
        mc:Ignorable="d"
        Title="Cashier"  
        Icon="pack://application:,,,/Resources/Images/Cashier_logo.png"
        Style="{StaticResource WindowStyle}">
    <i:Interaction.Triggers>
        <i:EventTrigger EventName="Loaded">
            <i:InvokeCommandAction Command="{Binding Loaded}"/>
        </i:EventTrigger>
    </i:Interaction.Triggers>
    <Grid>
        <Frame Content="{Binding CurrentPage, UpdateSourceTrigger=PropertyChanged}"
               Style="{StaticResource FramePageStyle}"/>
    </Grid>
</Window>
