<Window x:Class="Cashier.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Cashier.Views"
        mc:Ignorable="d"
        Title="Cashier"  
        Icon="pack://application:,,,/Resources/Images/Cashier_logo.png"
        Style="{StaticResource WindowStyle}">
    <Grid>
        <Frame Content="{Binding CurrentPage, UpdateSourceTrigger=PropertyChanged}"
               Style="{StaticResource FramePageStyle}"
               Name="fr"/>
    </Grid>
</Window>
